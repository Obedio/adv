<div class="main">
	<div>
		<table>
		  <thead>
		    <tr>
		      <th class="title-tb">Processo</th>
		      <th class="title-tb">Cliente</th>
		      <th colspan="3"></th>
		    </tr>
		  </thead>

		  <tbody>
		    <% @lawsuits.each do |lawsuit| %>
		      <tr>
		        <td class="nro-processo"><%= link_to lawsuit.number, lawsuit %></td>
		        <td><%= link_to lawsuit.client.name, lawsuit.client %></td>
		    <% end %>
		  </tbody>
		</table>
		<div class="paginate"><%= will_paginate @lawsuits %></div>
	</div>
</div>