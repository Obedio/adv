<%= form_for(data_user) do |f| %>
  <% if data_user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(data_user.errors.count, "error") %> prohibited this data_user from being saved:</h2>

      <ul>
      <% data_user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :born %>
    <%= f.date_select :born %>
  </div>

  <div class="field">
    <%= f.label :sex %>
    <%= f.select(:sex, ['Masculino', 'Feminino'], {include_blank: true}) %>
  </div>

  <div class="field">
    <%= f.label :office_id %>
    <%= f.collection_select(:office_id, Office.all, :id, :name, {include_blank: true})%>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
