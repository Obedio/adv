<p id="notice"><%= notice %></p>

<h1>Shares</h1>

<table>
  <thead>
    <tr>
      <th>Número</th>
      <th>Descrição</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shares.each do |share| %>
      <tr>
        <td><%= link_to share.lawsuit.number, share.lawsuit %></td>
        <td><%= share.lawsuit.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>